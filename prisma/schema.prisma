// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("SUPABASE_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id     String  @id @db.Uuid
  avatar String?
  bio    String? @db.Text

  @@map("users")
}
